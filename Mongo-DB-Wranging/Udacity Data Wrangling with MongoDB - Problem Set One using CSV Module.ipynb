​
def test():
    datafile = os.path.join(DATADIR, DATAFILE)
    name, data = parse_file(datafile)
​
    assert name == "MOUNTAIN VIEW MOFFETT FLD NAS"
    assert data[0][1] == "01:00"
    assert data[2][0] == "01/01/2005"
    assert data[2][5] == "2"
